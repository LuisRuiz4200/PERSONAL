package vistas;

import javax.swing.*;

@SuppressWarnings("serial")
public class DlgConsultaIngresoAvio extends JFrame{
	private JTable table;
	private JTextArea textArea;
	
	public static void main (String[] args) {
		DlgConsultaIngresoAvio ventana = new DlgConsultaIngresoAvio();
		ventana.setVisible(true);
	}
	
	public DlgConsultaIngresoAvio() {
		
		this.setTitle("LISTA DE INGRESO DE AVIOS");
		this.setBounds(0,0,673,442);
		this.setLocationRelativeTo(this);
		SpringLayout springLayout = new SpringLayout();
		getContentPane().setLayout(springLayout);
		
		table = new JTable();
		springLayout.putConstraint(SpringLayout.NORTH, table, 384, SpringLayout.NORTH, getContentPane());
		springLayout.putConstraint(SpringLayout.WEST, table, 22, SpringLayout.WEST, getContentPane());
		springLayout.putConstraint(SpringLayout.SOUTH, table, 169, SpringLayout.NORTH, getContentPane());
		springLayout.putConstraint(SpringLayout.EAST, table, 594, SpringLayout.WEST, getContentPane());
		getContentPane().add(table);
		
		textArea = new JTextArea();
		springLayout.putConstraint(SpringLayout.NORTH, textArea, 0, SpringLayout.NORTH, getContentPane());
		springLayout.putConstraint(SpringLayout.WEST, textArea, 270, SpringLayout.WEST, getContentPane());
		springLayout.putConstraint(SpringLayout.SOUTH, textArea, -10, SpringLayout.SOUTH, getContentPane());
		springLayout.putConstraint(SpringLayout.EAST, textArea, 74, SpringLayout.EAST, getContentPane());
		getContentPane().add(textArea);
		
		
		
	}
}
