package vistas;

import javax.swing.*;

@SuppressWarnings("serial")
public class DlgConsultaAvio extends JDialog{
	
	public static void main (String[] args) {
		DlgConsultaAvio ventana = new DlgConsultaAvio();
		ventana.setVisible(true);
	}
	
	public DlgConsultaAvio() {
		setTitle("CONSULTA OP");};
	
	public DlgConsultaAvio(JFrame frame, boolean bandera) {
		
		//MODAL 
		super(frame, bandera);
		
		this.setTitle("PLANTILLA");
		this.setBounds(0,0,429,326);
		this.setLocationRelativeTo(this);
		this.getContentPane().setLayout(null);
	}
	
	
}
